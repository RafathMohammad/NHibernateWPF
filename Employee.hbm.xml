<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NHibernateWPF"
                   namespace="NHibernateWPF">
	<class name="NHibernateWPF.Employee">
		<id name="Id">
			<generator class="identity" />
		</id>
		<property name="Name" />
		<property name="JoiningDate" />
		<property name="Email" />
		<property name="MobileNumber" />
		<property name="QLId" />
	</class>
</hibernate-mapping>